      "source": [
        "n = int(input())"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "x, y = 1, 1\n",
        "\n",
        "lrud_list = input().split()\n",
        "\n",
        "for lrud in lrud_list:\n",
        "  if y > 1 and lrud == 'L':\n",
        "    y -= 1\n",
        "  elif y < n and lrud == 'R':\n",
        "    y += 1\n",
        "  elif x > 1 and lrud == 'U':\n",
        "    x -= 1\n",
        "  elif x < n and lrud =='D':\n",
        "    x += 1\n",
        "\n",
        "print(x, y)\n"
      ],
