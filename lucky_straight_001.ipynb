      "source": [
        "n = int(input())"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "n = str(n)\n",
        "left_num = n[:len(n)//2]\n",
        "right_num = n[len(n)//2:]\n",
        "\n",
        "left_sum = 0\n",
        "right_sum = 0\n",
        "\n",
        "for l, r in zip(left_num, right_num):\n",
        "  left_sum += int(l)\n",
        "  right_sum += int(r)\n",
        "\n",
        "if left_sum == right_sum:\n",
        "  print(\"LUCKY\")\n",
        "else:\n",
        "  print(\"READY\")"
      ],
