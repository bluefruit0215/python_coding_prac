    {
      "cell_type": "code",
      "source": [
        "zero_count = 0\n",
        "one_count = 0\n",
        "zero_flag = -1\n",
        "for s in string_list:\n",
        "  if s == '0' and zero_flag != 1:\n",
        "    zero_flag = 1\n",
        "    zero_count += 1\n",
        "  elif s == '1' and zero_flag != 0:\n",
        "    zero_flag = 0\n",
        "    one_count += 1\n",
        "\n",
        "print(min(one_count, zero_count))"
      ],
